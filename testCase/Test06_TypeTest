main {
    int int_a, int_b := 10;
    bool flag_a, flag_b := true;
    char char_a, char_b := 'c';
    string str_a, str_b := "testString";
	const int int_c_a := 3;
    write("please input an int for int_a:");
    read(int_a);
	write("please input a bool for flag_a:");
    read(flag_a);
	write("please input a char for char_a:");
    read(char_a);
	write("please input a string for string_a:");
    read(str_a);
	write(int_a);
    write(int_b);
    write(flag_a);
	write(flag_b);
	write(char_a);
    write(char_b);
	write(str_a);
    write(str_b);
    write("===============================================");
    write(int_a + int_b + 3 * (int_a / int_b));
    write(flag_a && flag_b);
	write(flag_a || flag_b);
    write("following are constant test");
	write(int_c_a);
}
